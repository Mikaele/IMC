<%= form_tag("/resolvida_enviar") do  %>
    <div>
     <p>Resolver Chamado: #<%= link_to @call.id%></p>
      Descrição <%= @call.description%>
    </div>
    <%= hidden_field_tag :id,@call.id%>
    <div class="col-md-4">
      <%= label_tag :h_trabalhadas %><br>
      <%= time_select(:h_trabalhadas,:h_trabalhadas)%>

    </div>
    <div class="col-md-12">
      <%= submit_tag 'Enviar', :class=>"btn btn-primary"%>
    </div>
<% end %>
